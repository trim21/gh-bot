{
  "name": "graphql",
  "version": "0.5.3",
  "description": "",
  "private": true,
  "type": "module",
  "main": "bin/main.ts",
  "packageManager": "yarn@1.22.19",
  "engines": {
    "node": "^18.0.0"
  },
  "scripts": {
    "format": "prettier --list-different -w ./",
    "tsc": "tsc --noEmit",
    "lint": "eslint --cache --ext ts,cjs,js ./",
    "start": "nodemon ./bin/main.ts",
    "test": "vitest --run",
    "vitest": "vitest",
    "run-file": "node --no-warnings --loader @esbuild-kit/esm-loader",
    "e": "node --no-warnings -r dotenv/config --loader @esbuild-kit/esm-loader",
    "prepare": "husky install"
  },
  "dependencies": {
    "@esbuild-kit/esm-loader": "^2.5.5",
    "@octokit/rest": "^20.0.1",
    "@octokit/webhooks": "^12.0.2",
    "@sinclair/typebox": "^0.28.20",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "pino": "^8.14.2",
    "pino-pretty": "^10.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.17.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@typescript-eslint/utils": "^5.60.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-rulesdir": "^0.2.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-jsdoc": "^0.4.2",
    "typescript": "~5.1.6"
  },
  "nodemonConfig": {
    "ignore": [
      "**.test.ts"
    ],
    "watch": [
      "bin",
      "lib",
      "routes",
      "config.yaml",
      "package.json",
      "tsconfig.json",
      ".env"
    ],
    "delay": 100,
    "execMap": {
      "ts": "node --no-warnings -r dotenv/config --loader @esbuild-kit/esm-loader"
    },
    "env": {
      "NODE_ENV": "development",
      "REF": "development"
    },
    "ext": "ts,js,json,html"
  },
  "author": "",
  "license": "MIT",
  "prettier": {
    "plugins": [
      "prettier-plugin-jsdoc"
    ],
    "tsdoc": true,
    "printWidth": 120,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  }
}
